{
  "project": "my-app",
  "container": {
    "host": "localhost",
    "port": 3333,
    "user": "deploy"
  },
  "runtime": "bun",
  "startCommand": "bun run src/main.ts",
  "ignore": [
    "node_modules/**",
    ".git/**",
    "*.log",
    "dist/**",
    ".env*",
    "*.test.ts",
    "coverage/**"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "repository": "https://github.com/FuturPanda/devver-rd.git"
}
